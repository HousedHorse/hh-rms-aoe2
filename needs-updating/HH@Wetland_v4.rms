<LAND_GENERATION>       /* main lands and seas */
base_terrain DLC_JUNGLEGRASS

create_player_lands {
  terrain_type DLC_JUNGLEGRASS
  land_percent 10
  base_size 10
  left_border   -2
  right_border  -2
  top_border    -2
  bottom_border -2
  border_fuzziness 20
  clumping_factor 20
}

create_land {
  terrain_type DLC_MANGROVESHALLOW
  left_border   20
  right_border  20
  top_border    20
  bottom_border 20
  border_fuzziness 20
  clumping_factor 20
}

<ELEVATION_GENERATION>  /* hills */
