  create_elevation 2 {
    base_terrain DLC_JUNGLEGRASS
    number_of_clumps 3
    number_of_tiles 500
    set_scale_by_groups
    set_scale_by_size
  }
else
  create_elevation 4 {
    base_terrain DLC_JUNGLEGRASS
    number_of_clumps 5
    number_of_tiles 1000
    set_scale_by_groups
    set_scale_by_size
  }
endif

